{
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "npx tsc",
    "start": "node --experimental-loader=extensionless --es-module-specifier-resolution=node --no-warning ./dist/index.js",
    "compile": "npm run clean && npx tsc --watch",
    "clean": "rimraf ./dist/",
    "start-dev": "node --experimental-loader=extensionless --es-module-specifier-resolution=node --no-warnings --watch ./dist/index.js",
    "start-dev-browserSync": "node -e \"setTimeout(() => process.exit(0), 2000)\"; node --no-warnings --watch bsync.mjs",
    "start-dev-ui": "concurrently -c cyan,green -n server,browserSync \"npm:start-dev\" \"npm:start-dev-browserSync\"",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.6",
    "@types/glob": "^8.1.0",
    "@types/ip": "^1.1.0",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.29.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "browser-sync": "^2.29.1",
    "chalk": "^5.2.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "config": "^3.3.9",
    "connect-redis": "^7.0.1",
    "connect-sqlite3": "^0.9.13",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "extensionless": "^1.1.5",
    "glob": "^8.1.0",
    "i": "^0.3.7",
    "ip": "^1.1.8",
    "joi": "^17.9.1",
    "jschart": "^1.0.8",
    "moment": "^2.29.4",
    "mssql": "^9.1.1",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.12"
  }
}
